{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "condition": {
            "type": "string"
          },
          "deviation_type": {
            "type": "string"
          },
          "level": {
            "type": "integer"
          },
          "designation": {
            "type": "string"
          }
        },
        "required": ["condition", "deviation_type", "level", "designation"]
      }
    }
  },
  "required": ["message", "data"]
}